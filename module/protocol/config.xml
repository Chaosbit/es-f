<configuration>

  <section name="Module">

    <section name="Protocol">

      <!--
        Also show empty logs
      -->
      <config name="EmptyLogs" type="bool">FALSE</config>

      <!--
        Reverse protocols at output like rcat
      -->
      <config name="ReverseLog" type="bool">TRUE</config>

      <!--
        Auto refresh protocol in "show" mode, in seconds, 0 disables auto refresh
      -->
      <config name="AutoRefresh" type="int">10</config>

      <!-- *******************************************************************
      **  Don't change from here
      ******************************************************************** -->

      <config name="LoginRequired" type="bool">TRUE</config>
      <config name="Mobile" type="bool">FALSE</config>

      <config name="Name">Show esniper processes</config>

      <config name="Author">Knut Kohl</config>
      <config name="Email">knutkohl@users.sourceforge.net</config>

      <config name="Version">0.5.0</config>

    </section>

  </section>

</configuration>
