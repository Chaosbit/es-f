        <seg>Last auction update</seg>
      </tuv>
    </tu>

    <tu tuid="Error">
      <tuv xml:lang="EN">
        <seg>Error</seg>
      </tuv>
    </tu>

    <tu tuid="EsniperEncounteredBug">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>ATTENTION: esniper encountered a bug. Please take a look into [%s] or use Module Logfiles!</seg>
      </tuv>
    </tu>

    <tu tuid="AuctionSaved">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>Auction [%1$s] saved.</seg>
      </tuv>
    </tu>

    <tu tuid="GroupSaved">
      <tuv xml:lang="EN">
        <seg>Group saved.</seg>
      </tuv>
    </tu>

    <tu tuid="ModuleNotFound">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>Module [%1$s] not found or not activated!</seg>
      </tuv>
    </tu>

    <tu tuid="InvalidItem">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>Invalid item number (%1$s) or [Buy-It-Now] article!</seg>
      </tuv>
    </tu>

    <tu tuid="DeleteInvalid">
      <tuv xml:lang="EN">
        <seg>Delete invalid item</seg>
      </tuv>
    </tu>

    <tu tuid="NoItem">
      <tuv xml:lang="EN">
        <seg>Got no item number!</seg>
      </tuv>
    </tu>

    <tu tuid="ToMuchProcesses">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>There are more than 1 running esniper for group [%1$s]!</seg>
      </tuv>
    </tu>

    <tu tuid="Version">
      <tuv xml:lang="EN">
        <seg>Version</seg>
      </tuv>
    </tu>

    <tu tuid="Layout">
      <tuv xml:lang="EN">
        <seg>Layout</seg>
      </tuv>
    </tu>

    <tu tuid="SelectNone">
      <tuv xml:lang="EN">
        <seg>-- none --</seg>
      </tuv>
    </tu>

    <tu tuid="SelectFromGroup">
      <tuv xml:lang="EN">
        <seg>- from group -</seg>
      </tuv>
    </tu>

    <tu tuid="EFAbigger">
      <tuv xml:lang="EN">
        <seg>Increase font size</seg>
      </tuv>
    </tu>

    <tu tuid="EFAreset">
