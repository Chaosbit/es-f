  <body>
    <tu tuid="Welcome">
      <tuv xml:lang="EN">
        <seg>Welcome</seg>
      </tuv>
    </tu>

    <tu tuid="Login">
      <tuv xml:lang="EN">
        <seg>Login</seg>
      </tuv>
    </tu>

    <tu tuid="LastUpdate">
