<configuration>

  <section name="Plugin">

    <section name="AutoRefresh">

      <!--
        Refresh interval in minutes, default 1 hour
      -->
      <config name="Interval" type="int">60</config>

      <!-- *******************************************************************
      **  Don't change from here
      ******************************************************************** -->

      <config name="Mobile" type="bool">FALSE</config>

      <!-- Plugin is only available if JS in browser is enabled -->
      <config name="RequireJS" type="bool">Automatic delete ended auctions</config>

      <config name="Name">Refresh page automatic</config>

      <config name="Category">Interface</config>

      <config name="Author">Knut Kohl</config>
      <config name="Email">knutkohl@users.sourceforge.net</config>

      <config name="Version">0.1.0</config>
    </section>

  </section>

</configuration>
