<tmx version="1.4">
  <header
   creationtool="es-f"
   creationtoolversion="1"
   datatype="PlainText"
   segtype="sentence"
   adminlang="EN"
   srclang="EN"
   o-tmf="ABC"
   changeid="Knut Kohl / knutkohl@users.sourceforge.net"
   changedate="20101129T213215Z"
   o-encoding="UTF-8"
  >
    <prop type="x-Namespace">Autoupdate</prop>
  </header>
  <body>
    <tu tuid="LatestAppVersion">
      <prop type="x-Format">html</prop>
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg><![CDATA[The newest <tt>|es|f|</tt> version is &nbsp; <tt> %1$s</tt>, you can upgrade!]]></seg>
      </tuv>
    </tu>

    <tu tuid="LatestAppRelease">
      <prop type="x-Format">html</prop>
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg><![CDATA[The newest <tt>|es|f|</tt> version is &nbsp; <tt> %1$s</tt><br>%2$s<br>You can <a href="%3$s">upgrade</a>!]]></seg>
      </tuv>
    </tu>

    <tu tuid="FilesUpdatable">
      <note>%$1s - ...</note>
      <tuv xml:lang="EN">
        <seg>There are %1$d file(s) ready for update!</seg>
      </tuv>
    </tu>

    <tu tuid="FileNotWritable">
      <tuv xml:lang="EN">
        <seg>File is not writable, required for automatic update!</seg>
      </tuv>
    </tu>

    <tu tuid="UpdateNow">
      <tuv xml:lang="EN">
        <seg>Update now</seg>
      </tuv>
    </tu>

    <tu tuid="UpdateFiles">
      <tuv xml:lang="EN">
        <seg>Update files:</seg>
      </tuv>
    </tu>

  </body>
</tmx>
